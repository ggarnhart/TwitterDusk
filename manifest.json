{
  "manifest_version": 2,
 
  "name": "Twitter Dusk",
  "version": "1.0.1",
  "description": "A twitter theme inspired by the twitter app night mode.",
    "permissions":[
        "https://twitter.com/",
        "tabs",
        "activeTab"
    ],
    "background":{
        "scripts": ["Scripts/execute.js"]
    },
  "browser_action": {
      "default_popup": "nightExtension.html",
      "default_icon": "Images/darkTwitter.png",
      "default_html": "nightExtension.html"
  }
}